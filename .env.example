# Supabase URL used by browser/client code
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase publishable/anon key used by browser/client code
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-or-publishable-key

# Supabase URL for server/worker usage
SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase service role key (server/worker only, never expose to client)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
